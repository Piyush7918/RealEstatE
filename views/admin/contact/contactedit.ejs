<%- include('../sidebar.ejs')%>
    <%-include('../header.ejs')%>


        <div class="content mt-3">

            <form method="post" action="/contactupdate/<%= coedit._id %>" enctype="multipart/form-data">

                <div class="mb-3">
                    <label for="">Email Id</label>
                    <input type="text" name="cemail" class="form-control" value="<%= coedit.cemail %>">
                </div>
                <div class="mb-3">
                    <label for="">Phone</label>
                    <input type="text" name="cphone" class="form-control" value="<%= coedit.cphone %>">
                </div>

                <div class="mb-3">
                    <label for="">Contact Address</label>
                    <input type="text" name="caddress" class="form-control" value="<%= coedit.caddress %>">
                </div>
                
                <div>
                    <button type="submit" class="btn btn-primary">Submit</button>
                </div>
            </form>
        </div>


        <%- include('../footer.ejs') %>
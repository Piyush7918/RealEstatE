<%- include('./header.ejs') %>



  <!-- ======= Intro Section ======= -->
  <div class="intro intro-carousel swiper position-relative">

    <div class="swiper-wrapper">
      <% s.forEach((item)=>{ %>
      <div class="swiper-slide carousel-item-a intro-item bg-image" style="background-image: url(<%=item.Simage.url%>)">
        <div class="overlay overlay-a"></div>
        <div class="intro-content display-table">
          <div class="table-cell">
            <div class="container">
              <div class="row">
                <div class="col-lg-8">
                  <div class="intro-body">
                    <p class="intro-title-top"><%= item.location %></p>
                    <h1 class="intro-title mb-4 ">
                      <span class="color-b"><%= item.address %> </span> <%= item.city %>
                    </h1>
                    <p class="intro-subtitle intro-price">
                      <a href="#"><span class="price-a">rent |  <%= item.price %></span></a>
                    </p>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>

      <% }) %> 
    </div>
    <div class="swiper-pagination"></div>
  </div><!-- End Intro Section -->



  <main id="main">


    <!-------------------------------------------- search start------------------------------------------------- -->
  <div class="container-fluid bg-success mb-5" style="padding: 25px;">
    <div class="container">
    
        <form action="/search" method="post">
          <div class="row">
        <div class="col-md-3">

        </div>
       
        <div class="col-md-8">
          <div class="row">
            <div class="col-md-6">
              <input type="text"  name="search" class="form-control border-2 py-2" placeholder=" Search Property By City , Area ">
            </div>
            <div class="col-md-6">
              <button type="submit"class="btn btn-danger border-2 py-2 px-5">Search</button>
            </div>
          </form>
           
          </div>        
            </div>
            <div class="col-md-3"></div> 
            
        </div>
      </form>
      </div>
    </div>

  <!----------------------------------------------Search end---------------------------------------------->


     <!-- ======= Services Section ======= -->
     <section class="section-services section-t8">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <div class="title-wrap d-flex justify-content-between">
              <div class="title-box">
                <h2 class="title-a">Our Services</h2>
              </div>
            </div>
          </div>
        </div>
        <div class="row">
          <% se.forEach((os)=>{ %> 
          <div class="col-md-4">
            <div class="card-box-c foo">
              <div class="card-header-c d-flex">
                <div class="card-box-ico">
                  <span class="bi bi-cart"></span>
                </div>
                <div class="card-title-c align-self-center">
                  <h2 class="title-c"><%= os.sertitle %></h2>
                </div>
              </div>
              <div class="card-body-c">
                <p class="content-c">
                  <%= os.serdescription %>
                </p>
              </div>
            </div>
          </div>
          <% }) %>
        </div>
      </div>
    </section><!-- End Services Section -->

    <!-- ======= Latest Properties Section ======= -->
    <section class="section-property section-t8">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <div class="title-wrap d-flex justify-content-between">
              <div class="title-box">
                <h2 class="title-a">Latest Properties</h2>
              </div>
              <div class="title-link">
                <a href="property-grid.html">All Property
                  <span class="bi bi-chevron-right"></span>
                </a>
              </div>
            </div>
          </div>
        </div>

        <div id="property-carousel" class="swiper">
          <div class="swiper-wrapper">
            <% p.forEach((prop)=>{ %> 
            <div class="carousel-item-b swiper-slide">
              <div class="card-box-a card-shadow">
                <div class="img-box-a">
                  <img src="<%= prop.fimage.url %>" alt="" style="width:400px;height:400px" class="img-a img-fluid">
                </div>
                <div class="card-overlay">
                  <div class="card-overlay-a-content">
                    <div class="card-header-a">
                      <h2 class="card-title-a">
                        <a href="property-single.html"><%= prop.plocation %></a>
                      </h2>
                    </div>
                    <div class="card-body-a">
                      <div class="price-box d-flex">
                        <span class="price-a">rent | <%= prop.pprice %></span>
                      </div>
                      <a href="/property_single" class="link-a">Click here to view
                        <span class="bi bi-chevron-right"></span>
                      </a>
                    </div>
                    <div class="card-footer-a">
                      <ul class="card-info d-flex justify-content-around">
                        <li>
                          <h4 class="card-info-title">Area</h4>
                          <span><%= prop.pareas %>
                          </span>
                        </li>
                        <li>
                          <h4 class="card-info-title">Beds</h4>
                          <span><%= prop.pbeds %></span>
                        </li>
                        <li>
                          <h4 class="card-info-title">Baths</h4>
                          <span><%= prop.pbaths %></span>
                        </li>
                        <li>
                          <h4 class="card-info-title">Garages</h4>
                          <span><%= prop.pgarages %></span>
                        </li>
                      </ul>
                    </div>
                  </div>
                </div>
              </div>
            </div><!-- End carousel item -->
            <% }) %> 
          </div>
        </div>
        <div class="propery-carousel-pagination carousel-pagination"></div>

      </div>
    </section><!-- End Latest Properties Section -->

    <!-- ======= Agents Section ======= -->
    <section class="section-agents section-t8">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <div class="title-wrap d-flex justify-content-between">
              <div class="title-box">
                <h2 class="title-a">Best Agents</h2>
              </div>
              <div class="title-link">
                <a href="agents-grid.html">All Agents
                  <span class="bi bi-chevron-right"></span>
                </a>
              </div>
            </div>
          </div>
        </div>
        <div class="row">
          <% ag.forEach((agent)=>{ %>  
          <div class="col-md-4">
            <div class="card-box-d">
              <div class="card-img-d">
                <img src="<%= agent.agentimage.url %>" alt="" style="width:400px;height:400px" class="img-d img-fluid">
              </div>
              <div class="card-overlay card-overlay-hover">
                <div class="card-header-d">
                  <div class="card-title-d align-self-center">
                    <h3 class="title-d">
                      <a href="agent-single.html" class="link-two"><%= agent.agentname %></a>
                    </h3>
                  </div>
                </div>
                <div class="card-body-d">
                  <p class="content-d color-text-a">
                    <%= agent.aboutagent %>
                  </p>
                  <div class="info-agents color-a">
                    <p>
                      <strong>Phone: </strong> <%= agent.agentphone %>
                    </p>
                    <p>
                      <strong>Email: </strong> <%= agent.agentemail %>
                    </p>
                  </div>
                </div>
                <div class="card-footer-d">
                  <div class="socials-footer d-flex justify-content-center">
                    <ul class="list-inline">
                      <li class="list-inline-item">
                        <a href="#" class="link-one">
                          <i class="bi bi-facebook" aria-hidden="true"></i>
                        </a>
                      </li>
                      <li class="list-inline-item">
                        <a href="#" class="link-one">
                          <i class="bi bi-twitter" aria-hidden="true"></i>
                        </a>
                      </li>
                      <li class="list-inline-item">
                        <a href="#" class="link-one">
                          <i class="bi bi-instagram" aria-hidden="true"></i>
                        </a>
                      </li>
                      <li class="list-inline-item">
                        <a href="#" class="link-one">
                          <i class="bi bi-linkedin" aria-hidden="true"></i>
                        </a>
                      </li>
                    </ul>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <% }) %>
        </div>
      </div>
    </section><!-- End Agents Section -->

    <!-- ======= Testimonials Section ======= -->
    <section class="section-testimonials section-t8 nav-arrow-a">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <div class="title-wrap d-flex justify-content-between">
              <div class="title-box">
                <h2 class="title-a">Testimonials</h2>
              </div>
            </div>
          </div>
        </div>

        <div id="testimonial-carousel" class="swiper">
          <div class="swiper-wrapper">
            <% t.forEach((test)=>{ %> 
            <div class="carousel-item-a swiper-slide">
              <div class="testimonials-box">
                <div class="row">
                  <div class="col-sm-12 col-md-6">
                    <div class="testimonial-img">
                      <img src="<%= test.clientimage.url %>" alt="" style="width:600px" class="img-fluid">
                    </div>
                  </div>
                  <div class="col-sm-12 col-md-6">
                    <div class="testimonial-ico">
                      <i class="bi bi-chat-quote-fill"></i>
                    </div>
                    <div class="testimonials-content">
                      <p class="testimonial-text">
                        <%= test.clientfeedback %>
                      </p>
                    </div>
                    <div class="testimonial-author-box">
                      <img src="<%= test.clientimage.url %>" alt="" class="testimonial-avatar">
                      <h5 class="testimonial-author"><%= test.clientname %></h5>
                    </div>
                  </div>
                </div>
              </div>
            </div><!-- End carousel item -->
            <% }) %>
          </div>
        </div>
        <div class="testimonial-carousel-pagination carousel-pagination"></div>

      </div>
    </section><!-- End Testimonials Section -->

  </main><!-- End #main -->

  <%- include("./footer.ejs" )%>